<%= form_for :daily_log, url: '/daily_log' do |f| %>
		<%= f.text_field "projId", value: @projId , style:"display:none;" %>

		<p>
			<%= f.label :date, "Date" %><br>
			<%= f.date_select :date, class: "required textField" %>
			<%#= f.text_field :startDate%>
		</p>
		
		<p>
			Number of People in the Crew:<br> 
			<%= f.text_field :crewCuont%>	
		</p>
		
		<p>
			Total Number of Hours:<br>
			<%= f.text_field :totalHours%>	
		</p>
		
		<p>
			Activity Description:<br>
			<%= f.text_area :activity, size: "60x12"%>	
		</p>


		<%= f.submit "Submit"%>
		
		

	
	
<% end %>