
<% if !log_entries.nil? and !log_entries.empty? %>
	<% i = 1%>
	<% for entry in log_entries %>
		<% if i < 0 %>
			<% backgroundColor = 'beige'%>
		<% else %>
			<% backgroundColor = 'white'%>
		<% end %>
		<%= render :partial => "show", :locals => {:event => entry, :backgroundColor => backgroundColor}%>
		<% i = i * -1%>
	<% end %>
<%else%>
	No Log Entries to Show.
<% end %>



<% if false and  !log_entries.nil? and !log_entries.empty? %>

	<% for entry in log_entries %>
		<% if entry.has_key? 'changeRequestNumber' %>
		
			<%= render :partial => "log_entry/change_request/change_request_show", :locals => {:changeRequest => entry }%>
		
		<% elsif entry["type"] == 'site activity report' %>
		
			<%= render :partial => "/log_entry/activity_report/activity_report_show", :locals => {:activityReport => entry}%>
		
		<% end %>

	<% end %>
<%else%>


<% end %>