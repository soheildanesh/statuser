<table>
	<tr>
		<td> Event Type </td>
		<td>Site ID </td>
		<td>Old Crew</td>
		<td>New Crew  </td>
		<td> Submitted by </td>
	</tr>
	<tr>
		<td> Crew Change </td>
		<td><%= crewChange['siteId'] %> </td>
		<td><%= crewChange['oldCrew'] %> </td>
		<td><%= crewChange['newCrew'] %> </td>
		
		<% if crewChange['person'].nil? %>
		<td> 
			unknown 
		</td>
		<%else %>
		<td>
			<%= link_to crewChange['person'], :controller => 'person', :action => 'show', :id => crewChange['person'] %>
		</td>
		<%end%>
		
		</td>
			
		<td>
			<%= link_to 'Delete', {action: 'destroy', id: crewChange['_id']}, {method: :delete, data: { confirm: "Are you sure?" }} %>
		</td>
		
	</tr>
	<br>
</table>