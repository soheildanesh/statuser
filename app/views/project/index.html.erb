
 <%= link_to 'New', {controller: 'project', action: 'new'}, {style: "color:blue;"} %>


<%if !@projects.nil? %>
	<%if !@projects.empty? %>
		<h1>All Projects:</h1>
		<% for entry in @projects %>
		    <%= render partial: 'show', locals: {project: entry} %> <br>
		<% end %>
	<% else %>
		<br>
		No projects in database
	<% end %>
<%end%>