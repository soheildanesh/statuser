

<h2> Search Projects:</h2>

<%= form_for :project, url: {controller: 'project', action: 'search', id:'dummy'}, method: 'get' do |f| %>
	
	<p>
		<%= f.label :projId3s, "3SN Project ID" %><br>
		<%= f.text_field :projId3s, :value => @projId3s, "class" => ""	%>
		<br>
	</p>
	
	<p>
		<%= f.label :customerId, "3SN Customer ID" %><br>
		<%= f.text_field :customerId, "class" => " customerInput"%>
	</p>
	
	<p>
		<%= f.label :program, "3SN Program" %><br>
		<%= f.text_field :program, "class" => " programInput"%>
	</p>
	
	
	<p>
		<%= f.label :projName, "3SN Project Name" %><br>
		<%= f.text_field :projName, "class" => ""%>
	</p>
	
	
	<p>
		<%= f.label :projType, "Project Type" %><br>
		<%= f.text_field :projType, "class" => " projectTypeInput"%>
	</p>
	
	<p>
		<%= f.label :operatorId, "3SN Operator ID" %><br>
		<%= f.text_field :operatorId, "class" => ""%>
	</p>
	
	<p>
		<div id="projManager">
			<%= f.label :projManager, "3SN Project Manager" %><br>
			<%#= f.text_field :projManager%>
			<%= f.text_field :projManager, "class" => "personInput ", style: 'width:100px; display:none;', size:10%>
		</div>
	</p>
	
	<p>
		<div id="projController">
			<%= f.label :projController, "3SN Project Controller" %><br>
			<%#= f.text_field :projController%>
			<%= f.text_field :projController, "class" => "personInput required"%>
		</div>
	</p>
	
	<% if false #not yet supported %>
		<p>
			<%= f.label :startDate, "Start Date" %><br>
			<%= f.date_select :startDate, class: "" %>
			<%#= f.text_field :startDate%>
		</p>
	
		<p>
			<%= f.label :endDate, "End Date" %><br>
			<%= f.date_select :endDate, class: "" %>
			<%#= f.text_field :endDate%>
		</p>
	<% end %>
	
	<%#= f.submit "Submit", class: 'disabledTillFilled' %>
	<%= f.submit "Submit", class: '' %>
	
<%end%>
<br>
