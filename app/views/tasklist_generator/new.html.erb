Upload Bid File: <br>


<%= form_for 'spreadSheet', url:{conroller: 'tasklist_generator', action:'uploadSpreadSheet', id: @projectId.to_s }, method: "PATCH", style: "float:left;" do |f| %>
	<%= f.text_field "projectId", value: @projectId.to_s, style:"display:none;" %>
	<%= f.file_field "bidFile" %>
	<%= f.submit "Upload"%>
<% end %>
